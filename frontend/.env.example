# Frontend Environment Variables
# Copy this file to .env and customize the values for your setup

# API Base URL - where your Django backend is running
VITE_API_BASE_URL=http://localhost:8000/api

# Media Base URL - for serving images and media files
VITE_MEDIA_BASE_URL=http://localhost:8000

# For production, update these to your actual domain:
# VITE_API_BASE_URL=https://your-domain.com/api
# VITE_MEDIA_BASE_URL=https://your-domain.com

# For custom ports (example):
# VITE_API_BASE_URL=http://localhost:8080/api
# VITE_MEDIA_BASE_URL=http://localhost:8080